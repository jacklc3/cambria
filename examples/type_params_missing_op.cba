-- Test: handler instantiates $p but is missing an op (should fail)
with handler {
  $p -> Unique,
  return x -> return x,
  get a k -> k 42,
  ref x k -> k ()
} handle (
  declare !get : $p ~> Int.
  declare !set : $p * Int ~> Unit.
  declare !ref : Int ~> $p.
  do a <- !ref 0 in
  !set (a, !get a)
)
